{
  "printWidth": 80,
  "tabWidth": 2,
  "jsxBracketSameLine": false,
  "bracketSpacing": true,
  "arrowParens": "always",
  "singleQuote": true,
  "trailingComma": "es5",
  "semi": false,
  "newline-before-return": true,
  "no-duplicate-variable": [true, "check-parameters"],
  "no-var-keyword": true
}
